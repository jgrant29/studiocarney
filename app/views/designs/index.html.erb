            <div class="col-md-3">
              <div class="design-thumbnail" data-toggle="modal" data-target="#myModal" data-local="#myCarousel">
                <%= image_tag("ElevB_1.jpg", {class: "showme"}) %>
              </div>
            </div>
            <div class="col-md-3">
